{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["vite/client", "@cloudflare/workers-types/2023-07-01"],
    "noEmit": true
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
