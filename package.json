{
  "name": "vscode-remote-try-honox",
  "version": "0.1.0",
  "dependencies": {
    "@hono/zod-validator": "^0.3.0",
    "@prisma/client": "^5.20.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "hono": "^4.6.3",
    "honox": "^0.1.26",
    "tailwind-merge": "^2.5.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@cloudflare/workers-types": "^4.20241004.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@hono/vite-build": "^1.1.0",
    "@hono/vite-dev-server": "^0.16.0",
    "@tsconfig/strictest": "^2.0.5",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^6.2.3",
    "postcss": "^8.4.47",
    "prisma": "^5.20.0",
    "prisma-case-format": "^2.2.1",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "wrangler": "^3.80.0"
  },
  "packageManager": "pnpm@9.12.0",
  "private": true,
  "scripts": {
    "build": "vite build --mode client && vite build",
    "deploy": "$npm_execpath run build && wrangler pages deploy",
    "dev": "vite",
    "env": "dotenv -e .dev.vars",
    "fix": "run-s fix:biome fix:prisma-case fix:prisma",
    "fix:biome": "biome check --write",
    "fix:prisma": "prisma format",
    "fix:prisma-case": "prisma-case-format --file prisma/schema.prisma",
    "generate": "prisma generate",
    "lint": "biome check",
    "prepare": "node .husky/install.mjs",
    "preview": "wrangler pages dev",
    "setup": "corepack enable pnpm",
    "typecheck": "tsc --noEmit"
  },
  "type": "module"
}
