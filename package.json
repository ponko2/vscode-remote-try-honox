{
  "name": "vscode-remote-try-honox",
  "version": "0.1.0",
  "dependencies": {
    "hono": "^4.3.1",
    "honox": "^0.1.15"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.2",
    "@cloudflare/workers-types": "^4.20240502.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@hono/vite-cloudflare-pages": "^0.2.5",
    "@hono/vite-dev-server": "^0.12.0",
    "@tsconfig/strictest": "^2.0.5",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "wrangler": "^3.53.1"
  },
  "packageManager": "pnpm@9.0.6",
  "private": true,
  "scripts": {
    "build": "tsc --noEmit && vite build --mode client && vite build",
    "deploy": "$npm_execpath run build && wrangler pages deploy ./dist",
    "dev": "vite",
    "fix": "biome check --apply .",
    "lint": "biome check .",
    "prepare": "is-ci || husky",
    "preview": "wrangler pages dev ./dist",
    "setup": "corepack enable pnpm"
  },
  "type": "module"
}
