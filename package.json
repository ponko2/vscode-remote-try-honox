{
  "name": "vscode-remote-try-honox",
  "version": "0.1.0",
  "dependencies": {
    "@hono/zod-validator": "^0.2.1",
    "@prisma/client": "^5.13.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "hono": "^4.3.1",
    "honox": "^0.1.15",
    "tailwind-merge": "^2.3.0",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.2",
    "@cloudflare/workers-types": "^4.20240502.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@hono/vite-cloudflare-pages": "^0.2.5",
    "@hono/vite-dev-server": "^0.12.0",
    "@tsconfig/strictest": "^2.0.5",
    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^7.4.1",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.2",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "prisma": "^5.13.0",
    "prisma-case-format": "^2.2.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "wrangler": "^3.53.1"
  },
  "packageManager": "pnpm@9.0.6",
  "private": true,
  "scripts": {
    "build": "tsc --noEmit && vite build --mode client && vite build",
    "deploy": "$npm_execpath run build && wrangler pages deploy ./dist",
    "dev": "vite",
    "env": "dotenv -e .dev.vars",
    "fix": "run-s fix:biome fix:prisma-case fix:prisma",
    "fix:biome": "biome check --apply .",
    "fix:prisma": "prisma format",
    "fix:prisma-case": "prisma-case-format --file prisma/schema.prisma",
    "generate": "prisma generate",
    "lint": "biome check .",
    "prepare": "is-ci || husky",
    "preview": "wrangler pages dev ./dist",
    "setup": "corepack enable pnpm"
  },
  "type": "module"
}
