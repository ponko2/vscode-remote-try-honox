{
  "name": "vscode-remote-try-honox",
  "version": "0.1.0",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.2",
    "@hono/zod-validator": "^0.7.6",
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hono": "^4.11.1",
    "honox": "^0.1.52",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@cloudflare/workers-types": "4.20251219.0",
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@hono/vite-build": "1.8.0",
    "@hono/vite-dev-server": "0.23.0",
    "@tailwindcss/vite": "4.1.18",
    "@tsconfig/strictest": "2.0.8",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "npm-run-all2": "8.0.4",
    "prisma": "7.2.0",
    "prisma-case-format": "2.2.1",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "wrangler": "4.56.0"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^24.11.1",
      "onFail": "download"
    }
  },
  "packageManager": "pnpm@10.26.1",
  "private": true,
  "scripts": {
    "build": "vite build --mode client && vite build",
    "deploy": "$npm_execpath run build && wrangler deploy",
    "dev": "vite",
    "fix": "run-s fix:biome fix:prisma-case fix:prisma",
    "fix:biome": "biome check --write",
    "fix:prisma": "prisma format",
    "fix:prisma-case": "prisma-case-format --file prisma/schema.prisma",
    "generate": "prisma generate",
    "lint": "biome check",
    "prepare": "node .husky/install.mjs",
    "preview": "wrangler dev",
    "setup": "corepack enable pnpm",
    "typecheck": "tsc --noEmit"
  },
  "type": "module"
}
