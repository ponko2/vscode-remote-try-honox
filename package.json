{
  "name": "vscode-remote-try-honox",
  "version": "0.1.0",
  "dependencies": {
    "@hono/zod-validator": "^0.7.0",
    "@prisma/client": "^6.11.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hono": "^4.8.4",
    "honox": "^0.1.43",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@cloudflare/workers-types": "4.20250704.0",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@hono/vite-build": "1.6.2",
    "@hono/vite-dev-server": "0.20.0",
    "@tailwindcss/vite": "4.1.11",
    "@tsconfig/strictest": "2.0.5",
    "dotenv-cli": "8.0.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "npm-run-all2": "8.0.4",
    "prisma": "6.11.1",
    "prisma-case-format": "2.2.1",
    "tailwindcss": "4.1.11",
    "typescript": "5.8.3",
    "vite": "7.0.2",
    "wrangler": "4.23.0"
  },
  "packageManager": "pnpm@10.12.4",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "sharp",
      "workerd"
    ]
  },
  "private": true,
  "scripts": {
    "build": "vite build --mode client && vite build && cp ./.assetsignore ./dist",
    "deploy": "$npm_execpath run build && wrangler deploy",
    "dev": "vite",
    "env": "dotenv -e .dev.vars",
    "fix": "run-s fix:biome fix:prisma-case fix:prisma",
    "fix:biome": "biome check --write",
    "fix:prisma": "prisma format",
    "fix:prisma-case": "prisma-case-format --file prisma/schema.prisma",
    "generate": "prisma generate",
    "lint": "biome check",
    "prepare": "node .husky/install.mjs",
    "preview": "wrangler dev",
    "setup": "corepack enable pnpm",
    "typecheck": "tsc --noEmit"
  },
  "type": "module"
}
