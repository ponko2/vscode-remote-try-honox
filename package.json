{
  "name": "vscode-remote-try-honox",
  "version": "0.1.0",
  "dependencies": {
    "@hono/zod-validator": "^0.7.4",
    "@prisma/client": "^6.19.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hono": "^4.10.4",
    "honox": "^0.1.52",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@cloudflare/workers-types": "4.20251106.1",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@hono/vite-build": "1.7.0",
    "@hono/vite-dev-server": "0.23.0",
    "@tailwindcss/vite": "4.1.17",
    "@tsconfig/strictest": "2.0.7",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "npm-run-all2": "8.0.4",
    "prisma": "6.19.0",
    "prisma-case-format": "2.2.1",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3",
    "vite": "7.2.1",
    "wrangler": "4.46.0"
  },
  "packageManager": "pnpm@10.20.0",
  "private": true,
  "scripts": {
    "build": "vite build --mode client && vite build",
    "deploy": "$npm_execpath run build && wrangler deploy",
    "dev": "vite",
    "fix": "run-s fix:biome fix:prisma-case fix:prisma",
    "fix:biome": "biome check --write",
    "fix:prisma": "prisma format",
    "fix:prisma-case": "prisma-case-format --file prisma/schema.prisma",
    "generate": "prisma generate",
    "lint": "biome check",
    "prepare": "node .husky/install.mjs",
    "preview": "wrangler dev",
    "setup": "corepack enable pnpm",
    "typecheck": "tsc --noEmit"
  },
  "type": "module"
}
